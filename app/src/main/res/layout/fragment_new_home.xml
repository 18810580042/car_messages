<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".fragment.NewHomeFragment">

    <include
        android:id="@+id/new_home_tal"
        layout="@layout/layout_tal_show" />

    <com.example.car_message.utils.MyMapView
        android:id="@+id/map"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/new_home_tal" />

    <RelativeLayout
        android:id="@+id/new_home_in"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_27"
        android:layout_alignParentBottom="true"
        android:background="@color/white">

        <ImageView
            android:layout_width="@dimen/dp_20"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:src="@mipmap/in_click" />
    </RelativeLayout>
</RelativeLayout>